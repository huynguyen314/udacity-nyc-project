{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-huy-project"
		},
		"dfAzureDataLakeGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'dfAzureDataLakeGen2'"
		},
		"dfAzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'dfAzureSqlDatabase'"
		},
		"dfAzureSynapseAnalytics_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'dfAzureSynapseAnalytics'"
		},
		"dfAzureDataLakeGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://project4storageacc.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-huy-project_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"dfAzureDataLakeGen2_accountKey": {
						"value": "[parameters('dfAzureDataLakeGen2_accountKey')]"
					},
					"dfAzureSqlDatabase_connectionString": {
						"value": "[parameters('dfAzureSqlDatabase_connectionString')]"
					},
					"dfAzureSynapseAnalytics_connectionString": {
						"value": "[parameters('dfAzureSynapseAnalytics_connectionString')]"
					},
					"dfAzureDataLakeGen2_properties_typeProperties_url": {
						"value": "[parameters('dfAzureDataLakeGen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "df-huy-project_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'df-huy-project_ArmTemplate_0')]"
			]
		}
	]
}